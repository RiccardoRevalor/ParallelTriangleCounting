ifeq ($(OS), linux)
main_v2: cuda_matrixmultiplication_v2.cu ../../utils/utils.cpp
	"$(NVCC)" cuda_matrixmultiplication_v2.cu ../../utils/utils.cpp -o main_v2	

else ifeq ($(OS), windows)
main_v2: cuda_matrixmultiplication_v2.cu ../../utils/utils.cpp
	"$(NVCC)" -ccbin="$(VS_PATH)" cuda_matrixmultiplication_v2.cu ../../utils/utils.cpp -o main_v2

endif