ifeq ($(OS), linux)
main_v2_2: cuda_edge_it_v2_2.cu ../../utils/utils.cpp ../../utils/matrixMath.cpp
	"$(NVCC)" cuda_edge_it_v2_2.cu ../../utils/utils.cpp ../../utils/matrixMath.cpp -o main_v2_2

else ifeq ($(OS), windows)
